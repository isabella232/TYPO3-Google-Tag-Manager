{
    "name": "aoe/google-tag-manager",
    "description": "Extension that creates an interface for Google Tag Manager",
    "type": "typo3-cms-extension",
    "homepage": "https://github.com/AOEpeople/TYPO3-Google-Tag-Manager",
    "license": "GPL-3.0",
    "support": {
        "email": "dev@aoe.com",
        "issues": "https://github.com/AOEpeople/TYPO3-Google-Tag-Manager/issues",
        "source": "https://github.com/AOEpeople/TYPO3-Google-Tag-Manager"
    },
    "authors": [
        {
            "name": "AOE GmbH",
            "email": "dev@aoe.com",
            "homepage": "http://www.aoe.com"
        }
    ],
    "config": {
        "secure-http": false
    },
    "require": {
        "php": ">=5.5.0",
        "typo3/cms-core": ">=6.2.0,<=7.6.99",
        "typo3/cms-extbase": "*",
        "typo3/cms-fluid": "*"
    },
    "require-dev": {
        "typo3/cms": "^6.2",
        "phpunit/phpunit": "4.8.*",
        "phpunit/phpcov": "2.0.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "sebastian/phpcpd": "2.0.*",
        "phpmd/phpmd": "2.2.*"
    },
    "autoload": {
        "psr-4": {
            "Aoe\\GoogleTagManager\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\CMS\\Core\\Tests\\": "typo3/sysext/core/Tests/"
        }
    },
    "replace": {
        "google_tag_manager": "*"
    },
    "keywords": [
        "TYPO3",
        "extbase",
        "google-tag-manager"
    ]
}
